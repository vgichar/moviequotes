haracter": "Larry", "characterId": "nm0004310"}, {"text": "Whopper!", "character": "Curly", "characterId": "nm0397219"}, {"text": "Who's in the half?", "character": "Larry", "characterId": "nm0004310"}, {"text": "Whopper!", "character": "Curly", "characterId": "nm0397219"}, {"text": "Who's in the stretch?", "character": "Larry", "characterId": "nm0004310"}, {"text": "Whopper!", "character": "Curly", "characterId": "nm0397219"}, {"text": "Who's the winner?", "character": "Larry", "characterId": "nm0004310"}, {"text": "Rosemont!", "character": "Curly", "characterId": "nm0397219"}, {"text": "Who's the winner?", "character": "Moe", "characterId": "nm0002935"}, {"text": "Rosemont.", "character": "Curly", "characterId": "nm0397219"}, {"text": "[Moe slaps him]", "character": null, "characterId": null}]}]}